<select class="selectpicker" data-style="btn-primary" name="itemdropdown">
<option value="Category">Category</option>
<option value="Cabinet">Cabinet</option>
<option value="Chair">Chair</option>
<option value="Sofa">Sofa</option>
<option value="Side Table">Side Table</option>
<option value="Table">Table</option>

</select>

  
<select class="selectpicker" data-style="btn-primary" name="conditiondropdown">
<option value="Condition">Condition</option>
<option value="Vintage">Vintage</option>
<option value="New">New</option>
<option value="Like New">Like New</option>
<option value="Excellent">Excellent</option>
<option value="Good">Good</option>
<option value="Fair">Fair</option>

</select>


<select class="selectpicker" data-style="btn-primary" name="branddropdown">
<option value="Brand">Brand</option>
<option value="IKEA">IKEA</option>
<option value="Pottery Barn">Pottery Barn</option>
<option value="Crate and Barrel">Crate and Barrel</option>
<option value="Herman Miller">Herman Miller</option>
<option value="Pier 1 Imports">Pier 1 Imports</option>
<option value="Target">Target</option>
<option value="Macy's">Macy's</option>
<option value="Krause">Krause</option>
<option value="World Market">World Market</option>
<option value="Scandinavian Designs">Scandinavian Designs</option>
<option value="None of the above">None of the above</option>
</select>


<select class="selectpicker" data-style="btn-primary" name="materialdropdown">
<option value="Material">Material</option>
<option value="Wood">Wood</option>
<option value="Glass">Glass</option>
<option value="Fabric">Fabric</option>
<option value="Leather">Leather</option>
<option value="Steel">Steel</option>
<option value="Plastic">Plastic</option>
<option value="None of the above">None of the above</option>
</select>

<select class="selectpicker" data-style="btn-primary" name="colordropdown">
